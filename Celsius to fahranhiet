#include<stdio.h>
int main()
{
    float celsius;
    printf("The temp in celsius is \n");
    scanf("%f",&celsius);
    float far= (celsius*1.8)+32;
    printf("Converting celsius into fahrenheit %f ",far);
    return 0;
}




#include<stdio.h>
int main()
{
    float celsius = 37, far;
    far = (celsius * 9/5)+32;
    printf("celsius to fahrenhiet %f",far);
    
    return 0;
}
