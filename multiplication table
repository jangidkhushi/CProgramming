#include<stdio.h>
int main()
{
    int n;
    printf("Enter the number of which u need to learn the table of %d \n", n);
    scanf("%d", &n);
for(int i=1; i; i++){
printf("%d *%d =%d \n", n, i, n*i  );
if(i==10)
{break;}
}
return 0;

}






#include<stdio.h>
int main()
{
    int n =0;
    printf("Enter the number of which u need to learn the table of %d \n", n);
    scanf("%d", &n);
for(int i=1; i<=10; i++){
printf("%d *%d =%d \n", n, i, n*i  );
}
return 0;

}
